    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 109",
    "not firefox <= 110",
    "not samsung <= 19",
    "not and_ff <= 110",
    "not firefox <= 111",
    "not ios_saf <= 14.8",
    "not edge <= 111",
    "not ios_saf <= 16.3",
    "not safari <= 16.3",
    "not samsung <= 20"
  ],
